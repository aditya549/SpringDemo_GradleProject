<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

<bean id="c" class="com.java.SpringDemo.Circle">
<property name="pointA" ref="pointA"></property>
<property name="pointB" ref="pointB"></property>
<constructor-arg ref="pointC"></constructor-arg>
<property name="names">
<list>
<value>Equilateral</value>
<value>isosilation</value>
<value>EqualSides</value>
</list>
</property>
</bean>

<bean id="pointA"  class="com.java.SpringDemo.Point">
<property name="x" value="0"></property>
<property name="y" value="0"></property>
</bean>
<bean id="pointB"  class="com.java.SpringDemo.Point">
<property name="x" value="1"></property>
<property name="y" value="2"></property>
</bean>
<bean id="pointC"  class="com.java.SpringDemo.Point">
<property name="x" value="3"></property>
<property name="y" value="4"></property>
</bean>
<bean id="shape" class="com.java.SpringDemo.Traingle"></bean>
</beans>